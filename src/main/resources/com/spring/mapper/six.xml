<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

  <mapper namespace="com.spring.mapper.SixMapper">
  	<select id="restcafe" resultType="com.spring.domain.W_DTO">
  		select * from with_pet where category3='카페' or '식당'
  	</select>
  
  	<select id="accom" resultType="com.spring.domain.W_DTO">
  		select * from with_pet where category3='펜션'
  	</select>
  	
  	<select id="cult" resultType="com.spring.domain.W_DTO">
  		select * from with_pet where category3='여행지' or '문예회관' or '미술관' or '박물관'
  	</select>
  	
  	<select id="hosphar" resultType="com.spring.domain.W_DTO">
  		select * from with_pet where category3='동물병원' or '동물약국'
  	</select>
  	
  	<select id="goods" resultType="com.spring.domain.W_DTO">
  		select * from with_pet where category3='반려동물용품'
  	</select>
  	
  	<select id="care" resultType="com.spring.domain.W_DTO">
  		select * from with_pet where category3='미용' or '위탁관리'
  	</select>
  
  </mapper>